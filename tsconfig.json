{
  "extends": "tsconfig-jc",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "assets/*": ["./assets/*"]
    }
  },
  "exclude": ["node_modules"]
}
